    "plyr": "^3.7.8",
    "prisma": "^5",
    "rctx-contextmenu": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-toolkit": "^7.0.17",
    "react-lazy-load-image-component": "^1.5.6",
    "react-scroll-to-top": "^3.0.0",
    "react-use": "^17.4.0",
    "react-virtuoso": "^4.2.0",
    "sass": "^1.60.0",
